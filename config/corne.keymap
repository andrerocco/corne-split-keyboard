    keymap {
        compatible = "zmk,keymap";

        DEFAULT {
            bindings = <
&kp TAB         &kp Q         &kp W  &kp E      &kp R           &kp T    &kp Y      &kp U  &kp I      &kp O      &kp P           &kp SEMICOLON
&kp BSPC        &kp A         &kp S  &kp D      &kp F           &kp G    &kp H      &kp J  &kp K      &kp L      &kp APOSTROPHE  &kp GRAVE
&kp LEFT_SHIFT  &kp LEFT_ALT  &kp Z  &kp X      &kp C           &kp V    &kp B      &kp N  &kp M      &kp COMMA  &kp PERIOD      &kp SLASH
                                     &kp LCTRL  &kp LEFT_SHIFT  &mo 1    &kp SPACE  &mo 2  &kp ENTER
            >;
        };

        LOWER {
            bindings = <
&kp TAB        &kp N1          &kp N2  &kp N3     &kp N4     &kp N5      &trans     &kp AMPERSAND  &kp ASTERISK  &kp LEFT_PARENTHESIS  &kp RIGHT_PARENTHESIS  &kp COLON
&kp BACKSPACE  &kp N6          &kp N7  &kp N8     &kp N9     &kp N0      &kp MINUS  &kp EQUAL      &trans        &kp LBKT              &kp RBKT               &kp TILDE
&trans         &kp UNDERSCORE  &trans  &kp MINUS  &kp EQUAL  &kp PLUS    &trans     &trans         &kp BSLH      &kp LESS_THAN         &kp GREATER_THAN       &kp QUESTION
                                       &trans     &trans     &trans      &kp SPACE  &mo 3          &trans
            >;
